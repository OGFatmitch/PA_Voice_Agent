# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Server Configuration
PORT=3000
NODE_ENV=development

# Voice Configuration
# Available voices: alloy, ash, ballad, coral, echo, fable, nova, onyx, sage, shimmer
# Leave VOICE_MODEL empty or unset to use random voice selection for each session
# Set a specific voice (e.g., VOICE_MODEL=echo) to use that voice consistently
VOICE_MODEL=
VOICE_SPEED=1.2

# Audio Recording Configuration
MAX_RECORDING_TIME=8

# Performance Optimizations (all enabled by default for better latency)
ENABLE_STREAMING_TTS=true
ENABLE_PARALLEL_PROCESSING=true
PRELOAD_RESPONSES=true

# File Storage
UPLOAD_DIR=./uploads
TEMP_DIR=./temp 